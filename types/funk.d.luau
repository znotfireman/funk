declare function bind<Self, Arg, Returns...>(
  self: Self,
  fn: (self: Self, ...Arg) -> Returns...,
  ...: Arg
): (...Arg) -> Returns...
declare function memoize<Args..., Returns...>(fn: (Args...) -> Returns...): (Args...) -> Returns...
declare function partial<Arg, Returns...>(fn: (...Arg) -> Returns..., ...: Arg): (...Arg) -> Returns...